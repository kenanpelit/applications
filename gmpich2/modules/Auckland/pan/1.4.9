#%Module

proc ModulesHelp { } {
    puts stderr {   gcc and GFortran based compiler toolchain,
 including MPICH2 for MPI support. - Homepage: (none)
    }
}

module-whatis {Description: gcc and GFortran based compiler toolchain,
 including MPICH2 for MPI support. - Homepage: (none)}

set root    /share/easybuild/RHEL6.3/westmere/software/gmpich2/1.4.9

conflict    gmpich2

if { ![is-loaded GCC/4.8.2] } {
    module load GCC/4.8.2
}

if { ![is-loaded MPICH2/3.0.4-GCC-4.8.2] } {
    module load MPICH2/3.0.4-GCC-4.8.2
}


setenv	EBROOTGMPICH2		"$root"
setenv	EBVERSIONGMPICH2		"1.4.9"
setenv	EBDEVELGMPICH2		"$root/easybuild/gmpich2-1.4.9-easybuild-devel"


