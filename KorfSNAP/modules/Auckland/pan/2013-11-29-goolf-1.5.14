}

prepend-path	PATH		$root/bin

setenv	EBROOTKORFSNAP		"$root"
setenv	EBVERSIONKORFSNAP		"2013-11-29"
setenv	EBDEVELKORFSNAP		"$root/easybuild/KorfSNAP-2013-11-29-goolf-1.5.14-easybuild-devel"

setenv	ZOE		"$root"

