    }
}

module-whatis {Description: Semi-HMM-based Nucleic Acid Parser - Homepage: http://korflab.ucdavis.edu/software.html}

set root    /share/easybuild/RHEL6.3/sandybridge/software/KorfSNAP/2013-11-29-goolf-1.5.14

