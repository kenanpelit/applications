#%Module1.0

module-whatis "WPS, the WRF Preprocessing System with MPI (version 3.3.1)"

module load netcdf/4.1.3-pgi
module load openmpi/1.6_pgi-12.5
setenv NETCDF /share/apps/lib/netcdf-4.1.3-pgi
setenv NCARG_ROOT /share/apps/ncl-6.0.0

setenv JASPERLIB /usr/lib64
setenv JASPERINC /usr/include/jasper

prepend-path PATH /share/apps/WPS-3.3.1-OMPI
